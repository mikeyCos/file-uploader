<form action="/file/upload" method="POST" enctype="multipart/form-data">
  <h2>Upload File(s)</h2>
  <ul>
    <li class="form-item">
      <label for="upload_file">Upload file</label>
      <input type="file" id="upload_file" name="upload_file" multiple>
    </li>
  </ul>
  <button type="submit">Upload</button>
</form>