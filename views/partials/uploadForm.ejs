<form action="/drive/upload/file" method="POST" onsubmit="onSubmit(event)" onclick="stopPropagation(event)" enctype="multipart/form-data" class="form-upload-files" data-form-type="file">
  <h2>Upload File(s)</h2>
  <ul>
    <li class="form-item">
      <label for="upload_files">Upload file</label>
      <input type="file" id="upload_files" name="upload_files" multiple>
    </li>
  </ul>
  <button type="button" onclick="closeDialog()">Cancel</button>
  <button type="submit">Upload</button>
</form>